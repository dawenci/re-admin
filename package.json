    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "execa": "^5.1.1",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "less": "^4.1.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.12",
    "npm-run-path": "^4.0.1",
    "prettier": "^2.3.2",
    "rescript": "^9.1.4",
    "rescript-webapi": "^0.1.1",
    "sass": "^1.39.0",
    "vite": "^2.5.6",
    "vite-plugin-imp": "^2.0.8"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
